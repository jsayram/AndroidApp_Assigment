<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_vegetables_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.chefd.mealprep.mealprep.Vegetables_Activity"
    tools:showIn="@layout/activity_vegetables_">

    <TextView
        android:text="@string/recipe_v_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="118dp"
        android:id="@+id/Vegetable_Textview" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/handsomeman"
        android:layout_marginTop="79dp"
        android:id="@+id/imageView_v1"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/Vegetable_Textview"
        android:layout_alignEnd="@+id/Vegetable_Textview" />

</RelativeLayout>
